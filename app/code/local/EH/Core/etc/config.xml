<config>
    <modules>
        <EH_Core>
            <version>1.0.0</version>
        </EH_Core>
    </modules>
    <global>
		<blocks>
			<eh_core>
				<class>EH_Core_Block</class>
			</eh_core>
		</blocks>
		<helpers>
            <eh_core>
                <class>EH_Core_Helper</class>
            </eh_core>
        </helpers>
        <models>
            <eh_core>
                <class>EH_Core_Model</class>
            </eh_core>
        </models>
    </global>
    <adminhtml>
        <events>
            <controller_action_predispatch>
                <observers>
                    <eh_feed>
                        <type>singleton</type>
                        <class>eh_core/feed_general</class>
                        <method>feedFetch</method>
                    </eh_feed>
                </observers>
            </controller_action_predispatch>
        </events>
    </adminhtml>
</config>
